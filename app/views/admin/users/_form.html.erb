<%=form_with(model: [:admin, user]) do |form|%>
    <div><%=form.label "First Name"%>
    <%=form.text_field :first_name%></div>
    <div><%=form.label "Last Name"%>
    <%=form.text_field :last_name%></div>
    <div><%=form.label "Email"%>
    <%=form.text_field :email%></div>
    <div><%=form.label "Password"%>
    <%= form.password_field :password, required: true %></div>
    <div><%=form.label "Confirm Password"%>
    <%= form.password_field :password_confirmation, required: true %></div>
    <div><%= form.check_box :is_admin_user %> Is admin user</div>
    <%=form.submit%>
<%end%>